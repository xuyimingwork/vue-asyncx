{
  "name": "vue-asyncx",
  "version": "1.10.0-beta.0",
  "description": "让异步像写诗的 Vue 3 组合式工具库",
  "type": "module",
  "types": "dist/vue-asyncx.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-asyncx.umd.cjs",
  "module": "./dist/vue-asyncx.js",
  "exports": {
    ".": {
      "import": "./dist/vue-asyncx.js",
      "require": "./dist/vue-asyncx.umd.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test": "pnpm run test:unit",
    "prepublish": "pnpm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "vue",
    "utils",
    "async",
    "vueuse",
    "hooks",
    "ahooks"
  ],
  "author": "Xu Yiming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyimingwork/vue-asyncx.git"
  },
  "homepage": "https://xuyimingwork.github.io/vue-asyncx/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vueuse/core": "^14.0.0",
    "es-toolkit": "^1.42.0",
    "jsdom": "^24.1.1",
    "oxlint": "^1.0.0",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  }
}
