{
  "name": "vue-asyncx",
  "version": "1.9.1",
  "description": "优雅好用的 Vue3 异步函数/数据管理工具",
  "type": "module",
  "types": "dist/vue-asyncx.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-asyncx.umd.cjs",
  "module": "./dist/vue-asyncx.js",
  "exports": {
    ".": {
      "import": "./dist/vue-asyncx.js",
      "require": "./dist/vue-asyncx.umd.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test": "pnpm run test:unit",
    "prepublish": "pnpm run build"
  },
  "keywords": [
    "vue",
    "utils",
    "async",
    "vueuse",
    "hooks",
    "ahooks"
  ],
  "author": "Xu Yiming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyimingwork/vue-asyncx.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vueuse/core": "^14.0.0",
    "es-toolkit": "^1.42.0",
    "jsdom": "^24.1.1",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  }
}
